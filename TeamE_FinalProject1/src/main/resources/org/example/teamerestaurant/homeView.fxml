<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="310.0" prefWidth="306.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.teamerestaurant.home">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Button onAction="#onMenuButtonClick" prefHeight="50.0" prefWidth="140.0" text="Menu">
      <font>
         <Font name="Copperplate Gothic Light" size="12.0" />
      </font></Button>

    <Label fx:id="menuText" prefWidth="140.0" translateY="-5.0" />
   <Button mnemonicParsing="false" onAction="#onAccountButtonClick" prefHeight="50.0" prefWidth="140.0" text="Account" translateY="-5.0">
      <font>
         <Font name="Copperplate Gothic Light" size="12.0" />
      </font></Button>
   <Label fx:id="accountText" prefWidth="140.0" translateY="-9.0" />
   <Button mnemonicParsing="false" onAction="#onReservationButtonClick" prefHeight="50.0" prefWidth="147.0" text="Reservations" translateY="-8.0">
      <font>
         <Font name="Copperplate Gothic Light" size="12.0" />
      </font></Button>
   <Label fx:id="reservationText" prefWidth="180.0" translateY="-10.0" />
   <Button mnemonicParsing="false" onAction="#onOrderButtonClick" prefHeight="50.0" prefWidth="140.0" text="Order Online" translateY="-20.0">
      <font>
         <Font name="Copperplate Gothic Light" size="12.0" />
      </font></Button>
   <Label fx:id="orderText" prefHeight="50.0" prefWidth="200.0" translateY="-39.0" />
   <Button mnemonicParsing="false" onAction="#onAdminButtonClick" prefHeight="22.0" prefWidth="149.0" text="ADMIN " translateY="-30.0">
      <font>
         <Font name="Copperplate Gothic Light" size="12.0" />
      </font>
   </Button>
   <Label />
</VBox>
